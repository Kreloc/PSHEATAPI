    <ViewDefinitions>

        <View>
            <Name>Approval</Name>
            <ViewSelectedBy>
                <TypeName>FRS_Approval#</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>Name</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>ParentID</PropertyName>
                            </ListItem>
                            <ListItem>
                                <Label>ParentCategory</Label>
                                <PropertyName>ParentLink_Category</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>RecId</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>Status</PropertyName>
                            </ListItem>
                            <ListItem>
                                <Label>Votes</Label>
                                <ScriptBlock>
                                    $votes = foreach ($vote in $_.FRS_ApprovalVoteTracking) {
                                        @($vote.OwnerFullName, $vote.Status)
                                    }
                                    $votes
                                </ScriptBlock>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>

        <View>
            <Name>ApprovalVoteTracking</Name>
            <ViewSelectedBy>
                <TypeName>FRS_ApprovalVoteTracking#</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>PrimaryParentID</PropertyName>
                            </ListItem>
                            <ListItem>
