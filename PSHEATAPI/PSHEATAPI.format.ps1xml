                            <ListItem>
                                <PropertyName>Owner</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>OwnerTeam</PropertyName>
                            </ListItem>
                            <ListItem>
                                <Label>TimeFrame</Label>
                                <ScriptBlock>
                                    "$($_.ScheduledStartDate) ~ $($_.ScheduledEndDate)"
                                </ScriptBlock>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>

